<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProgressFabButton">
        <attr name="pFabColor" format="color" />
        <attr name="pFabRippleColor" format="color" />
        <attr name="pFabCircleColor" format="color" />
        <attr name="pFabProgressIcon" format="reference" />
        <attr name="pFabFinalIcon" format="reference" />
        <attr name="pFabTotalProgress" format="float" />
        <attr name="pFabCurrentProgress" format="float" />
        <attr name="pFabStartingProgress" format="float" />
        <attr name="pFabStepSize" format="integer" />
    </declare-styleable>

    <declare-styleable name="TimerButton">
        <!--分割线的大小-->
        <attr name="timerSplitSize" format="dimension" />
        <!--进度条的大小-->
        <attr name="timerProgressSize" format="dimension" />
        <!--进度条计时的时间-->
        <attr name="timerProgressTime" format="integer" />
        <!--进度条的最小值-->
        <attr name="timerMinProgress" format="float" />
        <!--进度条的最大值-->
        <attr name="timerTotalProgress" format="float" />
        <!--进度条的颜色-->
        <attr name="timerProgressColor" format="color" />
        <!--分割线的颜色-->
        <attr name="timerSplitColor" format="color" />
        <!--虚拟进度条路径的颜色-->
        <attr name="timerVirtualProgressColor" format="color" />
        <!--是否显示虚拟的路径-->
        <attr name="timerShowVirtual" format="boolean"/>
    </declare-styleable>

</resources>